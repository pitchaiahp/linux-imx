/dts-v1/;
/plugin/;

&bluetooth_iw61x {
	status = "disabled";
};

&iw612_pwrseq {
	status = "disabled";
};

/* WIFI */
&usdhc1 {
	mmc-pwrseq = <>;

	brcmf: wifi@1 {
		reg = <1>;
		compatible = "brcm,bcm4329-fmac";
		laird,regdomain = "US";
	};
};
